{
  "manifest_version": 3,
  "name": "Cloudhood (Dev)",
  "version": "1.1.9",
  "action": {
    "icon": "src/assets/img/main-icon-38.png",
    "default_popup": "src/index.html",
    "default_icon": {
      "38": "src/assets/img/main-icon-38.png"
    },
    "default_title": "Modify Outgoing XHR/Fetch Request Headers"
  },
  "icons": {
    "16": "src/assets/img/main-icon-16.jpg",
    "32": "src/assets/img/main-icon-32.jpg",
    "48": "src/assets/img/main-icon-48.jpg",
    "128": "src/assets/img/main-icon-128.jpg"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "tabs",
    "contextMenus"
  ],
  "background": {
    "service_worker": "src/background.ts",
    "type": "module"
  }
}
