{
  "manifest_version": 2,
  "name": "Cloudhood",
  "version": "1.1.1",
  "browser_action": {
    "icon": "main-icon-38.png",
    "default_popup": "popup.html",
    "default_icon": {
      "38": "main-icon-38.png"
    },
    "default_title": "Modify Outgoing XHR/Fetch Request Headers"
  },
  "icons": {
    "16": "main-icon-16.jpg",
    "32": "main-icon-32.jpg",
    "48": "main-icon-48.jpg",
    "128": "main-icon-128.jpg"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "declarativeNetRequest"
  ],
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  }
}